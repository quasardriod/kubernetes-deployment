[worker]
k8s-w ansible_host=192.168.10.66 machine_type=worker
[master]
k8s-master ansible_host=192.168.10.221 machine_type=master

[lb]
k8s-lb ansible_host=192.168.10.220 machine_type=haproxy

[all:vars]
ansible_user=root
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=~/.ssh/id_rsa
